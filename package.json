{
  "name": "yuarpp",
  "productName": "yuarpp",
  "version": "1.0.0",
  "description": "Polls tfs for recently completed releases",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make-windows": "./node_modules/.bin/electron-packager . yuarpp --overwrite  --platform=win32 --arch=x64  --prune=true --out=release-builds --version-string.CompanyName=RR --version-string.FileDescription=RR --version-string.ProductName=yuarpp",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "ry",
    "email": "ryanrosello@hotmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "yuarpp"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "build": {
    "directories": {
      "buildResources": "resources"
    },
    "extraFiles": [
      "resources"
    ]
  },
  "dependencies": {
    "electron-log": "^4.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "node-notifier": "^7.0.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "copyfiles": "^2.2.0",
    "electron": "^8.2.1",
    "electron-reload": "^1.5.0",
    "mkdirp": "^1.0.4"
  }
}
